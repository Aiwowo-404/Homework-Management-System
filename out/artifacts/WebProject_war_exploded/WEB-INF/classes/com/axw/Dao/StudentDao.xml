<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.axw.Dao.StudentDao">
    <select id="getAllStudent" resultType="com.axw.Pojo.Student">
        select * from axw.student
    </select>
    <select id="getStudentById" resultType="com.axw.Pojo.Student">
        select * from axw.student where id = #{sid}
    </select>
</mapper>